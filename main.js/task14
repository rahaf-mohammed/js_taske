function removeElementsFromLeft(arr, count) {
  if (count >= arr.length) {
    return [];
  }
  
  return arr.slice(count);
}

// Example usage
const originalArray = [1, 2, 3, 4, 5, 6];
const numberOfElementsToRemove = 3;

const newArray = removeElementsFromLeft(originalArray, numberOfElementsToRemove);
console.log(newArray); // Output: [4, 5, 6]

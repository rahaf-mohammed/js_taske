function filterValuesFromArray(originalArray, valuesToRemove) {
  return originalArray.filter(item => !valuesToRemove.includes(item));
}

// Example usage
const inputArray = [1, 2, 3, 4, 5, 6];
const valuesToRemove = [2, 4];

const filteredArray = filterValuesFromArray(inputArray, valuesToRemove);
console.log(filteredArray); // Output: [1, 3, 5, 6]
